{
  "ci": {
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "csp-xss": ["warn"],
        "tap-targets": ["warn"],
        "is-crawlable": ["warn"],
        "unused-css-rules": ["warn"],
        "unused-javascript": ["warn"]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
